<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="common_mreo_2" name="(Загальна послуга) - Надання витягу з Єдиного державного реєстру МВС України" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="bankIdlastName" name="Ваше прізвище" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ваше ім'я" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По батькові" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdbirthDay" name="Ваша дата народження; в форматі ДД.ММ.РРРР" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Ваш контактний телефон" type="long" default="380" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="Ваш e-mail; На цей e-mail  Ви отримаєте результат обробки Вашого звернення" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="reqType" name="Оберіть тип запитуваної інформації" type="enum" required="true">
          <activiti:value id="phis" name="інформація про володіння транспортнними засобами фізичною особою"></activiti:value>
          <activiti:value id="jur" name="інформація про володіння транспортнними засобами юридичною особою"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="reqPhis" name="Оберіть тип пошуку фізичної особи (власника транспортного засобу)" type="enum">
          <activiti:value id="0" name=" "></activiti:value>
          <activiti:value id="1" name="пошук за індивідуальним податковим номером"></activiti:value>
          <activiti:value id="2" name="пошук за ПІБ та даті народження"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="reqJur" name="Оберіть тип пошуку юридичної особи (власника транспортного засобу)" type="enum">
          <activiti:value id="0" name=" "></activiti:value>
          <activiti:value id="1" name="пошук за ЭДРПОУ"></activiti:value>
          <activiti:value id="2" name="пошук за найменуванням юридичної особи"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="ownerLegalName" name="Повна назва юрдичної особи (власника); вводити ПРОПИСНИМИ літерами" type="string"></activiti:formProperty>
        <activiti:formProperty id="ownerEDRPOU" name="ЭДРПОУ юридичної особи ( власника)" type="string"></activiti:formProperty>
        <activiti:formProperty id="ownerLastName" name="Прізвище власника транспортного засобу (укр.); вводити ПРОПИСНИМИ літерами" type="string"></activiti:formProperty>
        <activiti:formProperty id="ownerFirstName" name="Ім'я власника (укр.); вводити ПРОПИСНИМИ літерами" type="string"></activiti:formProperty>
        <activiti:formProperty id="ownerMiddleName" name="По батькові власника (укр.); вводити ПРОПИСНИМИ літерами" type="string"></activiti:formProperty>
        <activiti:formProperty id="ownerBirthDateUnformated" name="Дата народження власника; Підказка: Натисніть на кнопку з назвою місяця (у верхній частині календаря) і перейдете в режим вибору необхідного місяця. Після цього натисніть на кнопку з обраним роком (у верхній частині календаря) і перейдете в режим вибору необхідного року" type="date"></activiti:formProperty>
        <activiti:formProperty id="ownerInn" name="Індивидуальний податковий номер власника транспортного засобу" type="string"></activiti:formProperty>
        <activiti:formProperty id="text" name="Зверніть увагу" type="label" default="Замовляючи послугу Ви надаєте згоду на обробку Ваших персональних даних та підтверджуєте їх актуальність."></activiti:formProperty>
        <activiti:formProperty id="markers" name="markersForValidations" type="markers" default="{}"></activiti:formProperty>
        <activiti:formProperty id="markers1" name="show_fields" type="markers" default="${markerService.loadFromFile('mvd/common_mreo_2_1.json')}"></activiti:formProperty>
        <activiti:formProperty id="markers2" name="set_fields" type="markers" default="${markerService.loadFromFile('mvd/common_mreo_2_2.json')}"></activiti:formProperty>
        <activiti:formProperty id="markers4" name="required_fields" type="markers" default="${markerService.loadFromFile('mvd/common_mreo_2_3.json')}"></activiti:formProperty>
        <activiti:formProperty id="ownerBirthDate" name="ownerBirthDateFormated" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="searchDetail" name="searchDetail" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="reqDetail" name="reqDetail" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="resultData" name="Результат  перевірки" type="invisible" default="&lt;th&gt;no responce&lt;/th&gt;"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <serviceTask id="servicetask1" name="Підтягування документів" activiti:delegateExpression="#{fileTaskUpload}"></serviceTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="servicetask1"></sequenceFlow>
    <boundaryEvent id="boundaryerror1" name="Error" attachedToRef="servicetask1">
      <errorEventDefinition></errorEventDefinition>
    </boundaryEvent>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow3" sourceRef="boundaryerror1" targetRef="endevent1"></sequenceFlow>
    <userTask id="usertask1" name="Перевірка (запит по фізичним особам)" activiti:candidateGroups="sales">
      <extensionElements>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По Батькові" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdbirthDay" name="Дата народження" type="string"></activiti:formProperty>
        <activiti:formProperty id="reqDetail" name="reqDetail" type="string"></activiti:formProperty>
        <activiti:formProperty id="ownerLegalName" name="Повна назва юрдичної особи (власника)" type="string"></activiti:formProperty>
        <activiti:formProperty id="ownerEDRPOU" name="ЭДРПОУ юридичної особи ( власника)" type="string"></activiti:formProperty>
        <activiti:formProperty id="ownerInn" name="Індивидуальний податковий номер власника транспортного засобу" type="string"></activiti:formProperty>
        <activiti:formProperty id="ownerFirstName" name="Ім'я власника" type="string"></activiti:formProperty>
        <activiti:formProperty id="ownerLastName" name="Прізвище власника" type="string"></activiti:formProperty>
        <activiti:formProperty id="ownerMiddleName" name="По Батькові власника" type="string"></activiti:formProperty>
        <activiti:formProperty id="ownerBirthDate" name="Дата народження власника" type="string"></activiti:formProperty>
        <activiti:formProperty id="resultData" name="Результат перевірки" type="textArea"></activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <endEvent id="endevent8" name="End"></endEvent>
    <serviceTask id="servicetask11" name="Повідомлення громадянину про обробку заяви" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string>noreply@igov.org.ua</activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string>Надання витягу з Єдиного державного реєстру МВС України</activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/mvd/_common_header.html]
 &lt;h3&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/h3&gt;
 &lt;p&gt;                                   
Ваше звернення успішно оброблено. &lt;br/&gt;
Запит ${searchDetail} &lt;br/&gt;

Результат обробки: &lt;br/&gt;
&lt;table cellspacing="0" cellpadding="7" bordercolor="#5CA3CB" border="1" style="color:#5CA3CB;"&gt;
${resultData} 
&lt;/table&gt;

[pattern/mail/mvd/_common_signature.html]
[pattern/mail/mvd/_common_feedback.html]
[pattern/mail/mvd/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <scriptTask id="scripttask1" name="Зміна формату дат/часу" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>execution.setVariable("ownerBirthDate", execution.getVariable("ownerBirthDateUnformated").format("dd.MM.yyyy"))</script>
    </scriptTask>
    <sequenceFlow id="flow44" sourceRef="servicetask11" targetRef="endevent8"></sequenceFlow>
    <sequenceFlow id="flow49" sourceRef="scripttask1" targetRef="scripttask2"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow51" name="${reqPhis=='2'}" sourceRef="exclusivegateway1" targetRef="scripttask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reqPhis=='2'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow52" name="${reqPhis!='2'}" sourceRef="exclusivegateway1" targetRef="scripttask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reqPhis!='2'}]]></conditionExpression>
    </sequenceFlow>
    <scriptTask id="scripttask2" name="set search info for mail" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>   	   	var req=execution.getVariable('reqDetail')
		var info=' '
if (req=='inn'){
info='по фізичній особі з ІПН '+execution.getVariable('ownerInn')}
if (req=='fio'){
info = 'по фізичній особі: '+execution.getVariable('ownerLastName')+' '+execution.getVariable('ownerFirstName')+' '+execution.getVariable('ownerMiddleName')+' ('+execution.getVariable('ownerBirthDate')+' )'}
if (req=='LegalName'){
info ='по юридичній особі: '+execution.getVariable('ownerLegalName')}
if (req=='EDRPOU'){
info ='по юридичній особі з ЭДРПОУ '+execution.getVariable('ownerEDRPOU')}
execution.setVariable('searchDetail', info)
      </script>
    </scriptTask>
    <sequenceFlow id="flow57" sourceRef="servicetask1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow58" sourceRef="scripttask2" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow59" sourceRef="usertask1" targetRef="servicetask11"></sequenceFlow>
  </process>
