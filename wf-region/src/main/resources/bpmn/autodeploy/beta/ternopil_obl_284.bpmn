var per1 = 'post'
if (execution.getVariable('organID') == 'TernOda'){
per1 = 'sobesternopil@gmail.com'
}
if (execution.getVariable('organID') == 'TernRda'){
per1 = 'uszntrda@ukt.net'
}
if (execution.getVariable('organID') == 'BorshRda'){
per1 = 'borshchiv@sobes-ter.gov.ua'
}
execution.setVariable('postV', per1)
	  </script>
