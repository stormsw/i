  <process id="dnepr_cnap_277" name="Дніпропетровськ - Отримання дубліката картки державної реєстрації договорів (контрактів) про спільну інвестиційну діяльність за участю іноземного інвестора" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="info" name="Зверніть увагу: ця послуга є платною, при заповненні полів треба буде завантажити документ, що засвідчує внесення плати." type="label" default="Ви можете здійснити оплату зручним для Вас способом за  &lt;a href=&quot;https://docs.google.com/document/d/1yiEIf6rFHUJ1Xv0qaTyvWOG_pDvHjyBN4O5W3SgILnA/edit&quot; target=&quot;_blank&quot;&gt;реквізитами&lt;/a&gt; "></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По Батькові" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні данні" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="date_today" name="Укажіть сьогоднішню дату" type="date" required="true"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Укажіть контактний номер телефону" type="string" default="+380" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-Mail адреса для зворотнього зв'язку" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sID_Public_SubjectOrganJoin" name="Оберіть свій район або місто" type="select" default="0"></activiti:formProperty>
        <activiti:formProperty id="bankId_scan_passport" name="Сканована копія паспорту" type="file" default="завантажується автоматично" required="true"></activiti:formProperty>
        <activiti:formProperty id="press" name="Опублікована в офіційній пресі об’ява про визнання недійсною втраченої картки державної реєстрації договору (контракту)" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="payments" name="Оригінал документу, що засвідчує сплату збору за видачу картки і дубліката картки державної реєстрації договору (контракту)" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="text" name="Зверніть увагу" type="label" default="Замовляючи послугу Ви надаєте згоду на обробку Ваших персональних даних та підтверджуєте їх актуальність."></activiti:formProperty>
        <activiti:formProperty id="rNumber" name="номер району" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="rName" name="район" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="remail" name="пошта району" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="group" name="група району" type="invisible"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <userTask id="usertask3" name="рішення" activiti:candidateGroups="dnepr_cnap_vilnogirskT">
      <extensionElements>
        <activiti:formProperty id="rName" name="район" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="clfio" name="Автор звернення" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні данні" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="date_today" name="Укажіть сьогоднішню дату" type="date" writable="false"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Укажіть контактний номер телефону" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-Mail адреса для зворотнього зв'язку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="decision_2" name="прийняте рішення" type="enum" required="true">
          <activiti:value id="accept" name="дублікат готов"></activiti:value>
          <activiti:value id="reject" name="відхилити звернення (обов'язково впишіть коментар нижче)"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="comment_2" name="У разі відхилення внесіть, будь ласка, причину" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="rishen" name="Завантажте копію дубліката" type="file"></activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
        <activiti:taskListener event="create" delegateExpression="${fileTaskInheritance}">
          <activiti:field name="aFieldInheritedAttachmentID">
            <activiti:expression>${bankId_scan_passport},${press};${payments}</activiti:expression>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <serviceTask id="servicetask4" name="Повідомлення співробітника держоргану" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${remail}</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Нове звернення: Отримання дубліката картки державної реєстрації договорів (контрактів) про спільну інвестиційну діяльність за участю іноземного інвестора]]></activiti:string>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
&lt;strong&gt;Шановний(-а) колего!&lt;/strong&gt;&lt;br /&gt;&lt;br /&gt;
У Вас в черзі на обробку нове звернення.&lt;br /&gt;
Заявник: ${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}&lt;br /&gt;
Район: enum{[area]}&lt;br /&gt;
Відпрацюйте це звернення на &lt;a href="https://region.igov.org.ua" target="_blank"&gt;порталі&lt;/a&gt;
[pattern/mail/_common_signature.html]
[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask6" name="повідомлення громадянина про відхилення звернення" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Звернення на тему: Отримання дубліката картки державної реєстрації договорів (контрактів) про спільну інвестиційну діяльність за участю іноземного інвестора]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
&lt;strong&gt;Шановний(-а), ${bankIdfirstName} ${bankIdmiddleName}&lt;/strong&gt;&lt;br /&gt;
На жаль, Ваше звернення щодо отримання дубліката картки державної реєстрації договорів (контрактів) про спільну інвестиційну діяльність за участю іноземного інвестора відхилено через недостатню або неправильну інформацію.&lt;br /&gt;
${comment_2}&lt;br /&gt;&lt;br /&gt;
[pattern_dictonary:dnepr_reg_cnap.csv:value{[rNumber]}:5]&lt;br /&gt;
Звертаємо увагу, Ви можете заповнити заявку повторно на порталі &lt;a href="https://igov.org.ua/" target="_blank"&gt;iGov.org.ua&lt;/a&gt;
[pattern/mail/_common_signature_start.html]
[pattern_dictonary:dnepr_reg_cnap.csv:value{[rNumber]}:2]&lt;br /&gt;
[pattern/mail/_common_signature_end.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask7" name="повідомлення громадянина про готовність дублікату" activiti:delegateExpression="#{MailTaskWithAttachments}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>"${email}"</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Звернення на тему: Отримання дубліката картки державної реєстрації договорів (контрактів) про спільну інвестиційну діяльність за участю іноземного інвестора]]></activiti:string>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression>${rishen}</activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
&lt;strong&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/strong&gt;&lt;br /&gt;&lt;br /&gt;
Дублікат картки державної реєстрації договорів (контрактів) про спільну інвестиційну діяльність за участю іноземного інвестора готовий (сканована копія знаходиться у додатку до листа).&lt;br /&gt;
Ви або уповноважена Вами особа (при наявності довіреності) можете отримати дублікат собисто під підпис у ЦНАПі.&lt;br /&gt;
&lt;br /&gt;
При собі треба мати:&lt;br /&gt;
1. Документ, що посвідчує особу.&lt;br /&gt;
2. Для уповоноваженої особи - довіренність.&lt;br /&gt;
3. Роздруковану заяву (знаходиться у додатку до листа), яку ви подавали в електронному вигляді, засвідчену особистим  підписом  керівника  юридичної  особи, фізичної особи - підприємця  або  уповноваженої  ними  особи.&lt;br /&gt;
&lt;br /&gt;
4. Опубліковану в офіційній пресі об’ява про визнання недійсною втраченої картки державної реєстрації договору (контракту).&lt;br /&gt;
5. Оригінал документу, що засвідчує сплату збору за видачу картки і дубліката картки державної реєстрації договору (контракту).&lt;br /&gt;
Адреса [pattern_dictonary:dnepr_reg_cnap.csv:value{[rNumber]}:2]&lt;br /&gt;
[pattern_dictonary:dnepr_reg_cnap.csv:value{[rNumber]}:3]&lt;br /&gt;
[pattern_dictonary:dnepr_reg_cnap.csv:value{[rNumber]}:5]&lt;br /&gt;
Графік роботи: [pattern_dictonary:dnepr_reg_cnap.csv:value{[rNumber]}:4]&lt;br /&gt;
[pattern/mail/_common_signature_start.html]
[pattern_dictonary:dnepr_reg_cnap.csv:value{[rNumber]}:2]&lt;br /&gt;	   
[pattern/mail/_common_signature_end.html]
[pattern/mail/_common_feedback.html]
[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <scriptTask id="scripttask1" name="Передача значений" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>
var per1 = 'remail'
var per2 = 'rNumber'
var per3 = 'group'
var per4 = 'rName'
if (execution.getVariable('sID_Public_SubjectOrganJoin') == 'prav'){
per1 = 'darja.grek@gmail.com'
per2 = '23'
per3 = 'dnepr_cnap_prav'
per4 = 'ЦНАП «Правобережний» (просп. Карла Маркса, 75)'
}
if (execution.getVariable('sID_Public_SubjectOrganJoin') == 'lev'){
per1 = 'darja.grek@gmail.com'
per2 = '24'
per3 = 'dnepr_cnap_lev'
per4 = 'ЦНАП «Лівобережний» (просп. Газети «Правда», 42)'
}

execution.setVariable('remail', per1)
execution.setVariable('rNumber', per2)
execution.setVariable('group', per3)
execution.setVariable('rName', per4)
	  </script>
    </scriptTask>
