			[pattern_dictonary:dnepr_reg_cnap.csv:value{[region]}:2]&lt;br&gt;
			[pattern/mail/_common_signature_end.html]
			[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow16" sourceRef="servicetask5" targetRef="usertask1"></sequenceFlow>
    <userTask id="usertask1" name="Перевірка вхідних даних" activiti:candidateGroups="cnap_${sID_Public_SubjectOrganJoin}">
